{
  "categories": [
    {
      "name": "Processor",
      "counters": [
        "% Processor Time"
      ],
      "instances": [
        "_total"
      ]
    },
    {
      "name": ".NET CLR Memory",
      "dynamic": true,
      "instance_regex": "^metr",
      "counters": [
        "# Gen 0 Collections",
        "# Gen 1 Collections",
        "# Gen 2 Collections",
        "# Bytes in all Heaps",
        "% Time in GC"
      ]
    },
    {
      "name": ".NET CLR LocksAndThreads",
      "dynamic": true,
      "counters": [
        "contention rate / sec",
        "total # of contentions"
      ]
    }
  ]
}
